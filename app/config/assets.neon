## Front module
front:
	css:
		"compiled/front.min.css":
			## Bootstrap
			- "components/bootstrap/dist/css/bootstrap.min.css"
			- "components/bootstrap/dist/css/bootstrap-theme.min.css"
			## Forms
			- "components/datetimepicker/jquery.datetimepicker.css"
			- "components/jquery.tagsinput/src/jquery.tagsinput.css"
			- "components/EasyAutocomplete/dist/easy-autocomplete.css"
			## Main
			- "css/flashes.css"
			- "css/spinner.css"
			- "css/main.css"
