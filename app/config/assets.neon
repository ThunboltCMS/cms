			- "components/jquery.inputmask/dist/min/inputmask/inputmask.regex.extensions.min.js"
			- "components/jquery.inputmask/dist/min/inputmask/jquery.inputmask.min.js"
			- "components/datetimepicker/build/jquery.datetimepicker.full.min.js"
			- "components/jquery.tagsinput/src/jquery.tagsinput.js"
			- "components/EasyAutocomplete/dist/jquery.easy-autocomplete.min.js"
			- "components/webchemistry-form-controls/dist/form.controls.js"
			## Ajax
			- "components/thunbolt/dist/thunbolt.js"
			- "components/nette.ajax.js/nette.ajax.js"
			- "js/ajax/spinner.js"
			## Main
			- "js/main.js"
